generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:master@localhost:5432/rental-pal?schema=public"
}

model Users {
  id          Int      @id @default(autoincrement())
  username    String
  phoneNumber String   @unique
  lastLogin   DateTime?
}
